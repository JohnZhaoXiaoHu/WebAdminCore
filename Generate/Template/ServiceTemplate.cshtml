@{
	var name = $"{@Model.EntityName}Service";
}
using SqlSugar;
using WebModel.Entitys;
using WebUtils.BaseService;

namespace WebService.Service
{
    /// <summary>
	/// @name
	/// </summary>
    public partial class @name : BaseService<@Model.EntityName>, @($"I{name}")
	{
		IBaseRepository<@Model.EntityName> dal;
		ISqlSugarClient db;
		public @($"{name}(IBaseService<{Model.EntityName}> _dal)")
		{
			dal = _dal;
			db = dal.Db;
		}
	}
}