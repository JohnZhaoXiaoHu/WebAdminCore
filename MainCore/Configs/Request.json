{
    //Http请求配置
    "HttpRequest": {
        "Https": false,
        "Cors": {
            "PolicyName": "CorsIpAccess",
            "EnableAllIPs": false,
            // 支持多个域名端口，注意端口号后不要带/斜杆：比如localhost:8000/，是错的
            // 注意，http://127.0.0.1:1818 和 http://localhost:1818 是不一样的
            "IPs": "http://127.0.0.1:2364,http://localhost:2364"
        }
    }
}